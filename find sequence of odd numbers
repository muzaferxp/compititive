const n = 10;
const arr = [2,2,2,1,6,1,1,1,0,3];

let max_occurence = 0;
let occurence = 0;

for(x = 0; x <= n; x++){
    if(arr[x] % 2 == 1)
        occurence += 1;
    else
        if(occurence > max_occurence){
            max_occurence = occurence;
            occurence = 0;
        }
}

console.log(max_occurance)
