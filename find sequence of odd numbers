const n = 10;
const arr = [2,2,2,1,6,1,1,1,0,3];

let max_occurance = 0;
let occurance = 0;

for(x = 0; x<= n; x ++){
    if(arr[x] % 2 == 1)
        occurance += 1;
    else
        if(occurance > max_occurance){
            max_occurance = occurance;
            occurance = 0;
        }
}

console.log(max_occurance)
